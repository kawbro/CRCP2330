/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in = in, out = n1);
    And(a = a, b = n1, out = a1);

    Not(in = in, out = n2);
    And(a = n2, b = b, out = a2);

    Or(a = a1, b = a2, out = out);

}